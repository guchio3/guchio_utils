{
    "diagnostic.errorSign": ">>",
    "diagnostic.warningSign": ">>",
    "diagnostic.infoSign": ">>",
    "suggest.noselect": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "languageserver": {
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": [
                "dockerfile"
            ],
            "args": [
                "--stdio"
            ]
        },
        "efm": {
            "command": "efm-langserver",
            "args": [],
            "filetypes": [
                "vim",
                "eruby",
                "markdown"
            ]
        },
        "sql": {
            "command": "sql-language-server",
            "args": [
                "up",
                "--method",
                "stdio"
            ],
            "filetypes": [
                "sql",
                "mysql"
            ]
        }
    }
}
